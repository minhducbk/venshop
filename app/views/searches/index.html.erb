<% set_meta_tags title: 'Order Venshop' %>
<% set_meta_tags keywords: %w[venshop Venshop search] %>
<div class = "container">
  <div class = "row">
    <div class = "col-md-offset-0 col-md-4">
      <%= link_to "Home", root_path%> > Search result
    </div>
  </div>
  <br />
  <div class = "row">
    <div class = "col-md-offset-0 col-md-3">
      <%= render "categories/_parts/catalog", { categories: @categories}%>
    </div>
    <div class = "col-md-offset-0 col-md-9">
      <div>
        <%= render "searches/_parts/result_items",
          { list_items: @list_items, key: @key}%>
      </div>
      <hr>
      <%= paginate @list_items %>
      <%= page_entries_info @list_items %>
    </div>
  </div>
</div>
