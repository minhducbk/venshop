<div class = "row">
  <div class = "header_catalog col-md-offset-0 col-md-12 ">
    <%= category.name %>
  </div>
</div>
<br />
<div class = "recommened_list col-md-offset-1">
  <% items.each do |item| %>
    <div class = "row">
      <div class = "col-md-2 col-md-offset-0">
        <img src = "<%= item.image_url%>" height = "71px" width = "38px">
      </div>
      <div class = "col-md-9 col-md-offset-0">
        <%= link_to item.name, category_item_path(item.category.id, item.id) %>
        <br />
        <strong> Price </strong>: $<%= item.price%>
      </div>
    </div>
    <br />
  <% end %>
</div>
