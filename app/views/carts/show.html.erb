<% content_for :head do %>
  <%= display_meta_tags site: 'Venshop', title: "Cart" %>
  <%= display_meta_tags keywords: %w[venshop Venshop cart amazon ] %>
<% end %>
<div class="container">
  <div class="row">
    <div class="col-md-offset-0 col-md-4">
      <%= link_to "Home", root_path%> > My cart
    </div>
  </div>
  <br />
  <div class="row">
    <div class="col-md-offset-0 col-md-3">
      <%= render "categories/_parts/catalog", { categories: @categories}%>
    </div>
    <div class="col-md-offset-0 col-md-9">
      <div>
        <%= render "carts/_parts/cart_items", { cart_items: @cart_items, cart: @cart,
          reload: @reload, subtotal: @subtotal}%>
      </div>
    </div>
  </div>
</div>
