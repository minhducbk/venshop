$("#quantity_<%= @item.id.to_s %>").attr("selected", <%= @cart_item_quantity %>);
$(".show_cart").html("<%= get_cart_quantity(@cart).to_s %> Cart");
$("#subtotal").html("$<%= @cart_subtotal.to_s %>");
$(".subtotal_quantity").attr("value", <%= @cart_subtotal %>);
<% if get_cart_quantity(@cart) > 1 %>
  $("#count_items").html("<%= get_cart_quantity(@cart).to_s %> items");
<% else %>
  $("#count_items").html("1 item");
<% end %>
